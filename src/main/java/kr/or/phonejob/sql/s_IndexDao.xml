<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="kr.or.phonejob.mobile.s_Dao.s_IndexDao">
	<select id="getUserIp" resultType="kr.or.phonejob.mobile.s_Dto.s_UserIpDto">
		select user_ip from USE_IP
	</select>

	<select id="getUserData" parameterType="kr.or.phonejob.mobile.s_Dto.s_UserIpDto" resultType="integer">
		select count(*) from admin where user_name=#{user_name} and user_phone=#{user_phone} 
	</select>
	
	<insert id="insertIpData" parameterType="kr.or.phonejob.mobile.s_Dto.s_UserIpDto">
		insert into use_ip values(#{ip_name}, #{user_ip}, ip_seq.nextval, SYSDATE)
	</insert>


</mapper>